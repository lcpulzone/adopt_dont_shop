<h2>For a new bestie:<br>Fill out the form below</h2>

<%= form_with model: @application, method: :post, local: true do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>
<br>
<%= f.label :street_address %>
<%= f.text_field :street_address %>
<br>
<%= f.label :city %>
<%= f.text_field :city %>
<br>
<%= f.label :state %>
<%= f.text_field :state %>
<br>
<%= f.label :zip_code %>
<%= f.number_field :zip_code %>
<br>
<%= f.submit %>
<% end %>
